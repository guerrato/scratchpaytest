{
  "name": "scrathpay",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run build && npm start \"",
    "build": "npm run format && tsc && ./build.sh",
    "start": "npm run build && node ./dist/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "typescript": "^5.0.4"
  }
}
