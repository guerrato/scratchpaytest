{
  "name": "scrathpay",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md}' --config ./.prettierrc",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run build && npm start \"",
    "build": "rm -rf ./dist && npm run format && tsc && ./build.sh",
    "start": "node ./dist/www",
    "test": "jest"
  },
  "dependencies": {
    "chance": "^1.1.11",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "morgan": "~1.9.1",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
